            android:name=".LocalActivity"
            android:label="Local Shopping Places" />
        <activity
            android:name=".HistoricalActivity"
            android:label="Historical Places" />
        <activity
            android:name=".RestaurantActivity"
            android:label="Restaurants" />
        <activity
            android:name=".Attraction"
            android:label="World Trade Park" />
        <activity
            android:name=".Temple"
            android:label="Temple" />
        <activity
            android:name=".Restaurant"
            android:label="Restaurants" />
        <activity
            android:name=".Historical"
            android:label="Historical Places" />
        <activity android:name=".Directions"  android:label="Directions"/>
        <activity android:name=".MallActivity" android:label="Mall"></activity>
    </application>

</manifest>